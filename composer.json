{
    "name": "hatchetaustralia/laravel-url-shortener",
    "description": "Powerful URL shortening tools in Laravel",
    "keywords": [
        "laravel",
        "lumen",
        "url",
        "shortening",
        "shortener",
        "bit.ly",
        "tinyURL",
        "shorte.st"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Choraimy Kroonstuiver",
            "homepage": "https://github.com/axlon"
        },
        {
            "name": "László Görög",
            "email": "contact@nergal.xyz"
        },
        {
            "name": "Matt Hare",
            "email": "hello@hatchet.com.au"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "guzzlehttp/promises": "^1.0",
        "guzzlehttp/psr7": "^2.1.0",
        "illuminate/support": "^9.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "illuminate/routing": "^9.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Hatchet\\UrlShortener\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hatchet\\UrlShortener\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "tests": "./vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Hatchet\\UrlShortener\\UrlShortenerServiceProvider"
            ]
        }
    }
}
